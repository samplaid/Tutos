package lu.wealins.common.dto.webia.services;

import java.util.ArrayList;
import java.util.List;

/**
 * @author NGA
 *
 */
public class TransactionTaxDetailsRequest {

	private List<TransactionTaxDTO> transactionTax = new ArrayList<>();
	private Boolean frenchTaxable;

	/**
	 * @return the transactionTax
	 */
	public List<TransactionTaxDTO> getTransactionTax() {
		return this.transactionTax;
	}

	/**
	 * @param transactionTax
	 *            the transactionTax to set
	 */
	public void setTransactionTax(List<TransactionTaxDTO> transactionTax) {
		this.transactionTax = transactionTax;
	}

	/**
	 * @return the frenchTaxable
	 */
	public Boolean getFrenchTaxable() {
		return frenchTaxable;
	}

	/**
	 * @param frenchTaxable the frenchTaxable to set
	 */
	public void setFrenchTaxable(Boolean frenchTaxable) {
		this.frenchTaxable = frenchTaxable;
	}
	
}
