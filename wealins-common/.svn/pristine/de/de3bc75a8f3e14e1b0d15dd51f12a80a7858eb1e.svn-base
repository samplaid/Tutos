package lu.wealins.common.dto.webia.services;

import java.util.ArrayList;
import java.util.Collection;

import com.fasterxml.jackson.annotation.JsonIgnoreProperties;

@JsonIgnoreProperties(ignoreUnknown = true)
public class TransferWrapperDTO {

	private Collection<TransferDTO> transferDTO  = new ArrayList<>();
	private String error;
	/**
	 * @return the transferDTO
	 */
	public Collection<TransferDTO> getTransferDTO() {
		return transferDTO;
	}
	/**
	 * @param transferDTO the transferDTO to set
	 */
	public void setTransferDTO(Collection<TransferDTO> transferDTO) {
		this.transferDTO = transferDTO;
	}
	/**
	 * @return the error
	 */
	public String getError() {
		return error;
	}
	/**
	 * @param error the error to set
	 */
	public void setError(String error) {
		this.error = error;
	}
	
	
	
}