package lu.wealins.common.dto.liability.services;

public class FundSearcherRequest extends PageableSearchRequest {

	private String fdsId;
	private String fundSubType;
	private String isinCode;
	private String accountRoot;
	private String iban;
	private String ibanOrAccountRoot;
	private String depositBank;
	private String assetManager;
	private String financialAdvisor;
	private String name;
	private Integer status;
	private Boolean excludeTerminated;

	public Integer getStatus() {
		return status;
	}

	public void setStatus(Integer status) {
		this.status = status;
	}

	public String getIbanOrAccountRoot() {
		return ibanOrAccountRoot;
	}

	public void setIbanOrAccountRoot(String ibanOrAccountRoot) {
		this.ibanOrAccountRoot = ibanOrAccountRoot;
	}

	public String getFinancialAdvisor() {
		return financialAdvisor;
	}

	public void setFinancialAdvisor(String financialAdvisor) {
		this.financialAdvisor = financialAdvisor;
	}

	public String getFdsId() {
		return fdsId;
	}

	public void setFdsId(String fdsId) {
		this.fdsId = fdsId;
	}

	public String getFundSubType() {
		return fundSubType;
	}

	public void setFundSubType(String fundSubType) {
		this.fundSubType = fundSubType;
	}

	public String getIsinCode() {
		return isinCode;
	}

	public void setIsinCode(String isinCode) {
		this.isinCode = isinCode;
	}

	public String getAccountRoot() {
		return accountRoot;
	}

	public void setAccountRoot(String accountRoot) {
		this.accountRoot = accountRoot;
	}

	public String getIban() {
		return iban;
	}

	public void setIban(String iban) {
		this.iban = iban;
	}

	public String getDepositBank() {
		return depositBank;
	}

	public void setDepositBank(String depositBank) {
		this.depositBank = depositBank;
	}

	public String getAssetManager() {
		return assetManager;
	}

	public void setAssetManager(String assetManager) {
		this.assetManager = assetManager;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public Boolean getExcludeTerminated() {
		return excludeTerminated;
	}

	public void setExcludeTerminated(Boolean excludeTerminated) {
		this.excludeTerminated = excludeTerminated;
	}

}
