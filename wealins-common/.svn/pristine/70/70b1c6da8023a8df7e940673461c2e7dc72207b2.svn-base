package lu.wealins.common.dto.liability.services;

import java.math.BigDecimal;
import java.util.Date;

public class FundValuationDTO {

	private String fundId;
	private String currency;
	private Date effectiveDate;
	private String policyId;
	private String policyStatusDescription;
	private BigDecimal unitsByFund;
	private BigDecimal unitsByPolicy;
	private BigDecimal value;
	private BigDecimal split;
	private Date priceDate;
	private BigDecimal price;

	/**
	 * @return the policyStatusDescription
	 */
	public String getPolicyStatusDescription() {
		return policyStatusDescription;
	}

	/**
	 * @param policyStatusDescription the policyStatusDescription to set
	 */
	public void setPolicyStatusDescription(String policyStatusDescription) {
		this.policyStatusDescription = policyStatusDescription;
	}

	public String getFundId() {
		return fundId;
	}

	public void setFundId(String fundId) {
		this.fundId = fundId;
	}

	public String getPolicyId() {
		return policyId;
	}

	public void setPolicyId(String policyId) {
		this.policyId = policyId;
	}

	public BigDecimal getUnitsByFund() {
		return unitsByFund;
	}

	public void setUnitsByFund(BigDecimal unitsByFund) {
		this.unitsByFund = unitsByFund;
	}

	public BigDecimal getUnitsByPolicy() {
		return unitsByPolicy;
	}

	public void setUnitsByPolicy(BigDecimal unitsByPolicy) {
		this.unitsByPolicy = unitsByPolicy;
	}

	public Date getEffectiveDate() {
		return effectiveDate;
	}

	public void setEffectiveDate(Date effectiveDate) {
		this.effectiveDate = effectiveDate;
	}

	public BigDecimal getSplit() {
		return split;
	}

	public void setSplit(BigDecimal split) {
		this.split = split;
	}

	public Date getPriceDate() {
		return priceDate;
	}

	public void setPriceDate(Date priceDate) {
		this.priceDate = priceDate;
	}

	public BigDecimal getPrice() {
		return price;
	}

	public void setPrice(BigDecimal price) {
		this.price = price;
	}

	public BigDecimal getValue() {
		return value;
	}

	public void setValue(BigDecimal value) {
		this.value = value;
	}

	public String getCurrency() {
		return currency;
	}

	public void setCurrency(String currency) {
		this.currency = currency;
	}

}
