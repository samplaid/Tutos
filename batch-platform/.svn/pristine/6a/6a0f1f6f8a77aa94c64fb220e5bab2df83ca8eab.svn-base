<?xml version="1.0" encoding="UTF-8"?>
<jboss-web>
  	<context-root>batch-platform</context-root>

  	<resource-ref>
		<!-- Name defined in ActivationSpec java class -->
		<res-ref-name>jdbc/WIODS</res-ref-name>
		<!-- Name defined in JBoss EAP JCA ResourceAdapter AdminObject -->
		<jndi-name>java:jboss/datasources/WIODS</jndi-name>
	</resource-ref>
	
	<resource-ref>
	    <res-ref-name>jms/artemis/queueConnectionFactory</res-ref-name>
	    <jndi-name>java:/jms/artemis/queueConnectionFactory</jndi-name>
  	</resource-ref>
</jboss-web>