.policy-transaction{

    .transactionsContainerDetails{
        margin: 0px 0px ;
        //font-size: 0.8em;
        //width: 62.8vw;
        border-color:transparent;
        border-style: solid;
        border-width: 0.1px,bold;
        padding: 0px 0px;
        //position: absolute;
        z-index: 4;
        box-sizing: border-box;
       // box-shadow: 0 1px 1px 0 rgba(0, 0, 0, 0.1), 0 3px 9px 0 rgba(0, 0, 0, 0.1);
       //background-color:  #e9e9e9;
       border-radius: 6px;
    }

    .datatable-row-detail {
        box-shadow: inset 0px 5px 4px rgba(0,0,0,0.4);
    }

    .expand-icon {
        padding: 10px;
        margin: -9px;
    }

    #transaction-details {
        fieldset {
            padding: 3px;
            border: 1px solid #ccc;
            border-radius: 7px;
            border: 2px groove threedface #444;
            background-color: #e9e9e9;
        }

        legend {
            border-style: none;
            border-width: 0;
            font-size: 13px;
            line-height: 20px;
            margin: 0;
            text-align: left;
            width: auto;
            margin-left: 10px;
            font-weight: bold;
            background-color: white;
            border-radius: 10px;
            padding: 0px 10px;
            box-shadow: 3px 3px 7px -2px rgba(0,0,0,0.3);
        }
    }
}