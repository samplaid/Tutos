package lu.wealins.liability.services.ws.rest.exception;

public class WssUpdateAgentException extends RuntimeException {

	private static final long serialVersionUID = -7477584359613337601L;

	private String code;

	public WssUpdateAgentException(String message) {
		super(message);
	}

	public WssUpdateAgentException(String message, String code) {
		super(message);
		this.code = code;
	}

	public String getCode() {
		return code;
	}

	public void setCode(String code) {
		this.code = code;
	}

	public WssUpdateAgentException(Throwable cause) {
		super(cause);
	}
}
