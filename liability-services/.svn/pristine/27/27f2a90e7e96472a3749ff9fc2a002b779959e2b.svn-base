package lu.wealins.liability.services.core.business.exceptions;

public class WorkflowItemServiceException extends RuntimeException {

	private static final long serialVersionUID = -8125476096040434832L;

	public WorkflowItemServiceException(String message) {
		super(message);
	}

	public WorkflowItemServiceException(String message, Throwable cause) {
		super(message, cause);
	}

}
