<?xml version="1.0" encoding="UTF-8"?>
<!-- Repository and Service layers -->
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

	<bean id="exchangeRateInjectionService" class="lu.wealins.liability.services.core.business.impl.ExchangeRateInjectionServiceImpl" />
	<bean id="calendarUtils" class="lu.wealins.liability.services.core.utils.CalendarUtils" />
	<bean id="messageUtils" class="lu.wealins.liability.services.core.utils.MessageUtils" />

	<bean id="exchangeRateRepository" name="mockExchangeRateRepository"
		class="org.mockito.Mockito" factory-method="mock">
		<constructor-arg
			value="lu.wealins.liability.services.core.persistence.repository.ExchangeRateRepository" />
	</bean>
	<bean id="holidayRepository" name="mockHolidayRepository" class="org.mockito.Mockito"
		factory-method="mock">
		<constructor-arg
			value="lu.wealins.liability.services.core.persistence.repository.HolidayRepository" />
	</bean>
	<bean id="currencyRepository" name="mockCurrencyRepository" class="org.mockito.Mockito"
		factory-method="mock">
		<constructor-arg
			value="lu.wealins.liability.services.core.persistence.repository.CurrencyRepository" />
	</bean>
	<bean id="messageSource" name="mockMessageSource" class="org.mockito.Mockito"
		factory-method="mock">
		<constructor-arg value="org.springframework.context.MessageSource" />
	</bean>

</beans>