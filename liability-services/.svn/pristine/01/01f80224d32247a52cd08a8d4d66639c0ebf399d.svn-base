package lu.wealins.liability.services.ws.rest.exception;

public class WssftrupdExportException extends RuntimeException {

	private static final long serialVersionUID = -7477584359613337601L;

	private String code;

	public WssftrupdExportException(String message) {
		super(message);
	}

	public WssftrupdExportException(String message, String code) {
		super(message);
		this.code = code;
	}

	public String getCode() {
		return code;
	}

	public void setCode(String code) {
		this.code = code;
	}

	public WssftrupdExportException(Throwable cause) {
		super(cause);
	}
}
