package lu.wealins.liability.services.core.persistence.entity;

import java.math.BigDecimal;
import java.util.Date;

/**
 * Pojo for the valorized orders for FID file extraction
 * 
 * @author xqt5q
 *
 */
public class ValorizedOrderForFIDEntity {


	
	/**
	 * Injection reference
	 */
	private Long injectionReference;
	
	/**
	 * Currency
	 */
	private String currency;
	
	/**
	 * Quantity
	 */
	private BigDecimal quantity;
	
	/**
	 * Price
	 */
	private BigDecimal price;
	
	/**
	 * Date
	 */
	private Date operationDate;
	
	/**
	 * eventType
	 */
	private Integer eventType;
	
	/**
	 * event label
	 */
	private String eventLabel;
	
	/**
	 * orderType
	 */
	private String orderType;
	
	/**
	 * Fund
	 */
	private String fund;
	
	/**
	 * The line id
	 */
	private String lineId;
	
	/**
	 * Transaction id
	 */
	private Long transactionId;
	
	/**
	 * account root
	 */
	private String accountRoot;
	
	/**
	 * fund currency
	 */
	private String fundCurrency;
	
	/**
	 * status transaction
	 */
	private String transactionStatus;
	
	/**
	 * status
	 */
	private short status;
	
	
	
	public ValorizedOrderForFIDEntity(){
		
	}
	
	/**
	 * 
	 * @param injectionReference
	 * @param currency
	 * @param quantity
	 * @param price
	 * @param operationDate
	 * @param eventType
	 * @param fund
	 */
	public ValorizedOrderForFIDEntity(Long injectionReference, String currency, BigDecimal quantity, BigDecimal price, Date operationDate, Integer eventType, String fund, String accountRoot, String fundCurrency) {
		super();

		this.injectionReference = injectionReference;
		this.currency = currency;
		this.quantity = quantity;
		this.price = price;
		this.operationDate = operationDate;
		this.eventType = eventType;
		this.fund = fund;
		this.accountRoot = accountRoot;
		this.fundCurrency = fundCurrency;
	}

	

	/**
	 * @return the injectionReference
	 */
	public Long getInjectionReference() {
		return injectionReference;
	}

	/**
	 * @param injectionReference the injectionReference to set
	 */
	public void setInjectionReference(Long injectionReference) {
		this.injectionReference = injectionReference;
	}

	/**
	 * @return the currency
	 */
	public String getCurrency() {
		return currency;
	}

	/**
	 * @param currency the currency to set
	 */
	public void setCurrency(String currency) {
		this.currency = currency;
	}

	/**
	 * @return the quantity
	 */
	public BigDecimal getQuantity() {
		return quantity;
	}

	/**
	 * @param quantity the quantity to set
	 */
	public void setQuantity(BigDecimal quantity) {
		this.quantity = quantity;
	}

	/**
	 * @return the price
	 */
	public BigDecimal getPrice() {
		return price;
	}

	/**
	 * @param price the price to set
	 */
	public void setPrice(BigDecimal price) {
		this.price = price;
	}

	/**
	 * @return the operationDate
	 */
	public Date getOperationDate() {
		return operationDate;
	}

	/**
	 * @param operationDate the operationDate to set
	 */
	public void setOperationDate(Date operationDate) {
		this.operationDate = operationDate;
	}

	/**
	 * @return the eventType
	 */
	public Integer getEventType() {
		return eventType;
	}

	/**
	 * @param eventType the eventType to set
	 */
	public void setEventType(Integer eventType) {
		this.eventType = eventType;
	}

	/**
	 * @return the fund
	 */
	public String getFund() {
		return fund;
	}

	/**
	 * @param fund the fund to set
	 */
	public void setFund(String fund) {
		this.fund = fund;
	}



	/**
	 * @return the accountRoot
	 */
	public String getAccountRoot() {
		return accountRoot;
	}



	/**
	 * @param accountRoot the accountRoot to set
	 */
	public void setAccountRoot(String accountRoot) {
		this.accountRoot = accountRoot;
	}



	/**
	 * @return the fundCurrency
	 */
	public String getFundCurrency() {
		return fundCurrency;
	}



	/**
	 * @param fundCurrency the fundCurrency to set
	 */
	public void setFundCurrency(String fundCurrency) {
		this.fundCurrency = fundCurrency;
	}

	public Long getTransactionId() {
		return transactionId;
	}

	public void setTransactionId(Long transactionId) {
		this.transactionId = transactionId;
	}

	public String getOrderType() {
		return orderType;
	}

	public void setOrderType(String orderType) {
		this.orderType = orderType;
	}

	

	public String getTransactionStatus() {
		return transactionStatus;
	}

	public void setTransactionStatus(String transactionStatus) {
		this.transactionStatus = transactionStatus;
	}

	public String getEventLabel() {
		return eventLabel;
	}

	public void setEventLabel(String eventLabel) {
		this.eventLabel = eventLabel;
	}

	public String getLineId() {
		return lineId;
	}

	public void setLineId(String lineId) {
		this.lineId = lineId;
	}

	public short getStatus() {
		return status;
	}

	public void setStatus(short status) {
		this.status = status;
	}

	
}	

