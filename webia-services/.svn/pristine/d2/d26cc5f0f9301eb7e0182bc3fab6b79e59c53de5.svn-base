<?xml version="1.0" encoding="UTF-8"?>
<!-- Repository and Service layers -->
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

	<bean id="priceInjectionService" class="lu.wealins.webia.services.core.business.impl.PriceInjectionServiceImpl" />
	<bean id="calendarUtils" class="lu.wealins.webia.services.core.utils.CalendarUtils" />
	<bean id="messageUtils" class="lu.wealins.webia.services.core.utils.MessageUtils" />

	<!-- Mock Objects -->
	<bean id="fundPriceRepository" name="mockFundPriceRepository"
		class="org.mockito.Mockito" factory-method="mock">
		<constructor-arg
			value="lu.wealins.webia.services.core.persistence.repository.FundPriceRepository" />
	</bean>
	<bean id="fundRepository" name="mockFundRepository" class="org.mockito.Mockito"
		factory-method="mock">
		<constructor-arg
			value="lu.wealins.webia.services.core.persistence.repository.FundRepository" />
	</bean>
	<bean id="fundTransactionRepository" name="mockFundTransactionRepository"
		class="org.mockito.Mockito" factory-method="mock">
		<constructor-arg
			value="lu.wealins.webia.services.core.persistence.repository.FundTransactionRepository" />
	</bean>
	<bean id="holidayRepository" name="mockHolidayRepository" class="org.mockito.Mockito"
		factory-method="mock">
		<constructor-arg
			value="lu.wealins.webia.services.core.persistence.repository.HolidayRepository" />
	</bean>
	<bean id="messageSource" name="mockMessageSource" class="org.mockito.Mockito"
		factory-method="mock">
		<constructor-arg value="org.springframework.context.MessageSource" />
	</bean>

</beans>