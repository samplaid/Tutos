package lu.wealins.webia.services.core.components;

import java.math.BigDecimal;
import java.util.List;

import lu.wealins.webia.services.core.persistence.entity.CommissionToPayEntity;

public class CommissionToPayWrapper {
	private CommissionToTypeTuple commissionToTypeTuple;
	private BigDecimal totalAmount;
	private List<CommissionToPayEntity> commissionToPayEntities;
	private Long transferId;


	public CommissionToTypeTuple getCommissionToTypeTuple() {
		return commissionToTypeTuple;
	}

	public void setCommissionToTypeTuple(CommissionToTypeTuple commissionToTypeTuple) {
		this.commissionToTypeTuple = commissionToTypeTuple;
	}

	public BigDecimal getTotalAmount() {
		return totalAmount;
	}

	public void setTotalAmount(BigDecimal totalAmount) {
		this.totalAmount = totalAmount;
	}

	/**
	 * @return the commissionToPayEntities
	 */
	public List<CommissionToPayEntity> getCommissionToPayEntities() {
		return commissionToPayEntities;
	}

	/**
	 * @param commissionToPayEntities the commissionToPayEntities to set
	 */
	public void setCommissionToPayEntities(List<CommissionToPayEntity> commissionToPayEntities) {
		this.commissionToPayEntities = commissionToPayEntities;
	}

	public Long getTransferId() {
		return transferId;
	}

	public void setTransferId(Long transferId) {
		this.transferId = transferId;
	}

}
